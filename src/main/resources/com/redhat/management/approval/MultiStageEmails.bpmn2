      <bpmn2:script><![CDATA[java.util.ArrayList<com.redhat.management.approval.Group> groups = (java.util.ArrayList<com.redhat.management.approval.Group>) kcontext.getVariable("groups");
String decision = (String) SignalContent.get("decision");

com.redhat.management.approval.Group currentGroup = groups.remove(0);

System.out.println("Now processing group: " + currentGroup.getName());

kcontext.setVariable("currentGroup", currentGroup);
kcontext.setVariable("groups", groups);
kcontext.setVariable("StageContent", EmailDispatcher.getStageContent(decision));]]></bpmn2:script>
