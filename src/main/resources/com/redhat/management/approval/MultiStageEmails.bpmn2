      <bpmn2:incoming>_5CD1F091-858F-4C73-A327-4F7ADBCD6F59</bpmn2:incoming>
      <bpmn2:outgoing>_3948D404-7EC6-4006-9483-C99CEC645818</bpmn2:outgoing>
      <bpmn2:script><![CDATA[System.out.println("Process approvers now ...");

com.redhat.management.approval.Request request = (com.redhat.management.approval.Request) kcontext.getVariable("request");
com.redhat.management.approval.Group currentGroup = (com.redhat.management.approval.Group) kcontext.getVariable("currentGroup");
String decision = (SignalContent == null) ? "undecided" : (String)SignalContent.get("decision");

java.util.ArrayList<com.redhat.management.approval.Approver> approvers = currentGroup.getApprovers(); 
java.util.ArrayList<com.redhat.management.approval.Stage> stages = (java.util.ArrayList<com.redhat.management.approval.Stage>) kcontext.getVariable("stages");


com.redhat.management.approval.EmailDispatcher dispatcher = new com.redhat.management.approval.EmailDispatcher();

kcontext.setVariable("BackOfficeHeaders", dispatcher.getHeaders());
kcontext.setVariable("BackOfficeBody", dispatcher.getContent(approvers, currentGroup, stages, request));
kcontext.setVariable("BackOfficeUrl", dispatcher.getUrl());
kcontext.setVariable("StageContent", dispatcher.getStageContent(decision));]]></bpmn2:script>
    </bpmn2:scriptTask>
