      <bpmn2:script><![CDATA[String content = (String) kcontext.getVariable("StageContent");
com.redhat.management.approval.Group currentGroup = (com.redhat.management.approval.Group) kcontext.getVariable("currentGroup");
java.util.ArrayList<com.redhat.management.approval.Stage> stages = (java.util.ArrayList<com.redhat.management.approval.Stage>) kcontext.getVariable("stages");
com.redhat.management.approval.Request request = (com.redhat.management.approval.Request) kcontext.getVariable("request");

com.redhat.management.approval.Stage stage = EmailDispatcher.getCurrentStage(currentGroup, stages);
kcontext.setVariable("StageUrl", EmailDispatcher.getStageUrl(stage));
String id = request.createSysadminIdentity();

kcontext.setVariable("identityHeaders", id);

System.out.println(stage);
System.out.println("Stage content: " + content);
System.out.println("Stage url: " + EmailDispatcher.getStageUrl(stage));
System.out.println("stage identity: " + (String) kcontext.getVariable("identityHeaders"));]]></bpmn2:script>
    </bpmn2:scriptTask>
