      <bpmn2:script><![CDATA[System.out.println("Process groups now ...");

com.redhat.management.approval.Request request = (com.redhat.management.approval.Request) kcontext.getVariable("request");
java.util.ArrayList<com.redhat.management.approval.Group> groups = (java.util.ArrayList<com.redhat.management.approval.Group>) kcontext.getVariable("groups");

com.redhat.management.approval.Group currentGroup = groups.remove(0);

System.out.println("Now processing group: " + currentGroup.getName());


kcontext.setVariable("currentGroup", currentGroup);
